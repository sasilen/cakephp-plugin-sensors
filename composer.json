{
    "name": "sasilen/sensors",
    "description": "Sesults plugin for CakePHP 4",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "~4.1.0",
        "muffin/tags": "cake-4.x"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sasilen\\Sensors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sasilen\\Sensors\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
